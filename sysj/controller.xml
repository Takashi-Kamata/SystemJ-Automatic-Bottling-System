<System xmlns="http://systemjtechnology.com">
	<SubSystem Name="SS1" Local="true">
	
	
		<ClockDomain Name="ControllerCD" Class="Controller">
			
			<iSignal Name="request"  Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="30000" />
			
			<iSignal Name="FirstLiquidDone"  Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="30001" />
			<iSignal Name="SecondLiquidDone"  Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="30001" />
			<iSignal Name="ThirdLiquidDone"  Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="30001" />
			<iSignal Name="FourthLiquidDone"  Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="30001" />
			
			<iSignal Name="Ready"  Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="30001" />
			
			
			<iChannel Name="FirstLiquid" 	From="PlantCD.FirstLiquid" />
			<iChannel Name="SecondLiquid" 	From="PlantCD.SecondLiquid" />
			<iChannel Name="ThirdLiquid"  	From="PlantCD.ThirdLiquid" />
			<iChannel Name="FourthLiquid" 	From="PlantCD.FourthLiquid" />
			<iChannel Name="LiquidFlow"  	From="PlantCD.LiquidFlow" />


			<iChannel Name="FirstLiquidAmount" 	From="PlantCD.FirstLiquidAmount" />
			<iChannel Name="SecondLiquidAmount" From="PlantCD.SecondLiquidAmount" />
			<iChannel Name="ThirdLiquidAmount" 	From="PlantCD.ThirdLiquidAmount" />
			<iChannel Name="FourthLiquidAmount" From="PlantCD.FourthLiquidAmount" />
			
			
			<oSignal Name="LiquidFillerDone" To="PlantCD.LiquidFillerDone" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10001" />
			<oSignal Name="Liquid1On" To="PlantCD.Liquid1On" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10001" />
			<oSignal Name="Liquid2On" To="PlantCD.Liquid2On" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10001" />
			<oSignal Name="Liquid3On" To="PlantCD.Liquid3On" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10001" />
			<oSignal Name="Liquid4On" To="PlantCD.Liquid4On" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10001" />
			
			<oSignal Name="Counter" To="PlantCD.Counter" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10001" />
			
			<oSignal Name="RequestE" To="UI.RequestE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			
			
		
		</ClockDomain>
		
		
		<ClockDomain Name="PlantCD" Class="Plant">

			<iSignal Name="recipe"  Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10000" />
			<iSignal Name="enable" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10000" />
			
			<iSignal Name="LiquidFillerDone"       Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />	
			<iSignal Name="Liquid1On" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="Liquid2On" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="Liquid3On" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="Liquid4On"  Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			
			<iSignal Name="Counter"  Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			
			
			<oChannel Name="FirstLiquid" 	To="ControllerCD.FirstLiquid" />
			<oChannel Name="SecondLiquid" 	To="ControllerCD.SecondLiquid" />
			<oChannel Name="ThirdLiquid"  	To="ControllerCD.ThirdLiquid" />
			<oChannel Name="FourthLiquid" 	To="ControllerCD.FourthLiquid" />
			<oChannel Name="LiquidFlow"  	To="ControllerCD.LiquidFlow" />
			
			
			<oChannel Name="FirstLiquidAmount" 	To="ControllerCD.FirstLiquidAmount" />
			<oChannel Name="SecondLiquidAmount" To="ControllerCD.SecondLiquidAmount" />
			<oChannel Name="ThirdLiquidAmount" 	To="ControllerCD.ThirdLiquidAmount" />
			<oChannel Name="FourthLiquidAmount" To="ControllerCD.FourthLiquidAmount" />
			
			<oSignal Name="FirstLiquidAmountE" Class="com.systemj.ipc.TCPSender" IP="127.0.0.1" Port="20001" />
			<oSignal Name="SecondLiquidAmountE" Class="com.systemj.ipc.TCPSender" IP="127.0.0.1" Port="20002" />
			<oSignal Name="ThirdLiquidAmountE" Class="com.systemj.ipc.TCPSender" IP="127.0.0.1" Port="20003" />
			<oSignal Name="FourthLiquidAmountE" Class="com.systemj.ipc.TCPSender" IP="127.0.0.1" Port="20004" />
			
			<oSignal Name="Ready" To="ControllerCD.Ready" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="30001" />
			
			<oSignal Name="FirstLiquidDone" To="ControllerCD.FirstLiquidDone" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="30001" />
			<oSignal Name="SecondLiquidDone" To="ControllerCD.SecondLiquidDone" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="30001" />
			<oSignal Name="ThirdLiquidDone" To="ControllerCD.ThirdLiquidDone"  Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="30001" />
			<oSignal Name="FourthLiquidDone" To="ControllerCD.FourthLiquidDone"  Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="30001" />
			
			<oSignal Name="Liquid1OnE" To="UI.Liquid1OnE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="Liquid2OnE" To="UI.Liquid2OnE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="Liquid3OnE" To="UI.Liquid3OnE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="Liquid4OnE" To="UI.Liquid4OnE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="LiquidFillerDoneE" To="UI.LiquidFillerDoneE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="RemoveE" To="UI.RemoveE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="TestE" Class="com.systemj.ipc.TCPSender" IP="127.0.0.1" Port="20069" />
		</ClockDomain>
		
		
	</SubSystem>
</System>